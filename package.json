    "axios": "^1.10.0",
    "expo": "~53.0.17",
    "expo-auth-session": "~6.2.1",
    "expo-crypto": "~14.1.5",
    "expo-font": "~13.3.2",
    "expo-image-picker": "~16.1.4",
